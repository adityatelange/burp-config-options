{
    "proxy":{
        "http_history_display_filter":{
            "by_annotation":{
                "show_only_commented_items":false,
                "show_only_highlighted_items":false
            },
            "by_file_extension":{
                "hide_items":[
                    "woff2"
                ],
                "hide_specific":true,
                "show_items":[
                    "asp",
                    "aspx",
                    "jsp",
                    "php"
                ],
                "show_only_specific":false
            },
            "by_listener":{
                "port":""
            },
            "by_mime_type":{
                "show_css":false,
                "show_flash":true,
                "show_html":true,
                "show_images":true,
                "show_other_binary":true,
                "show_other_text":true,
                "show_script":true,
                "show_xml":true
            },
            "by_request_type":{
                "hide_items_without_responses":false,
                "show_only_in_scope_items":true,
                "show_only_parameterized_requests":false
            },
            "by_status_code":{
                "show_2xx":true,
                "show_3xx":true,
                "show_4xx":true,
                "show_5xx":true
            }
        }
    }
}